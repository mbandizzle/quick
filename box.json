{
    "name":"quick",
    "version":"2.4.1",
    "author":"Ortus Solutions",
    "location":"coldbox-modules/quick#v2.4.1",
    "homepage":"https://github.com/coldbox-modules/quick",
    "documentation":"https://github.com/coldbox-modules/quick",
    "repository":{
        "type":"git",
        "URL":"https://github.com/coldbox-modules/quick"
    },
    "bugs":"https://github.com/coldbox-modules/quick/issues",
    "slug":"quick",
    "shortDescription":"A ColdBox ORM Engine",
    "description":"A ColdBox ORM Engine",
    "scripts":{
        "format": "cfformat dsl/**/*.cfc,extras/**/*.cfc,interceptors/**/*.cfc,models/**/*.cfc,tests/specs/**/*.cfc --overwrite",
        "format:check": "> cfformat dsl/**/*.cfc,extras/**/*.cfc,interceptors/**/*.cfc,models/**/*.cfc,tests/specs/**/*.cfc --check",
        "postVersion":"package set location='coldbox-modules/quick#v`package version`'"
    },
    "type":"modules",
    "dependencies":{
        "qb":"^6.1.0",
        "str":"^1.0.0",
        "testbox":"^3.1.0+339"
    },
    "devDependencies":{
        "coldbox":"be",
        "testbox":"^2.4.0+80",
        "cfcollection":"^3.6.1"
    },
    "installPaths":{
        "testbox":"testbox/",
        "coldbox":"tests/resources/app/coldbox/",
        "qb":"modules/qb/",
        "str":"modules/str/",
        "cfcollection":"modules/cfcollection/"
    },
    "testbox":{
        "reporter":"json",
        "runner":"http://127.0.0.1:52482/tests/runner.cfm",
        "verbose":"false",
        "watchDelay":"1000"
    },
    "ignore":[
        "**/.*",
        "test",
        "tests",
        ".engine",
        ".travis.yml",
        ".cfformat.json",
        ".cfconfig.json",
        ".env.example",
        ".gitignore",
        "quick300.png",
        "server.json"
    ]
}
